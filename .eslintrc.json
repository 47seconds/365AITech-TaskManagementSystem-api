{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "json"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off"
  },
  "overrides": [
    {
      "files": ["**/*.json"],
      "parser": "json5-eslint-parser", 
      "rules": {
        
      }
    }
  ]
}
